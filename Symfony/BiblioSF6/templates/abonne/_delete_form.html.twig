<form method="post" action="{{ path('app_abonne_delete', {'id': abonne.id}) }}" onsubmit="return confirm('Etes vous sûr de vouloir supprimer cet abonné ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ abonne.id) }}">
    <button class="btn btn-danger mt-2">Supprimer</button>
</form>
